{
  "description": "block microsoft safelinks requests",
  "manifest_version": 2,
  "name": "microsoft safelink blocker",
  "version": "0.0.1",
  "homepage_url": "https://github.com/h43z/microsoft-safelink-blocker-extension",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "https://statics.teams.cdn.office.net/evergreen-assets/safelinks/1/atp-safelinks.html*",
    "https://safelinks.protection.outlook.com/*",
    "https://*.safelinks.protection.outlook.com/*",
    "https://outlook.office.com/mail/safelink.html*",
    "https://*.safelinks.protection.office365.us/*"
  ],
  "background": {
    "scripts": [
      "background-script.js"
    ]
  }
}
